<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    tools:context=".ProjectWebDesigActivity" >

    <ImageView
        android:id="@+id/acm_image"
        android:layout_width="140dp"
        android:layout_height="99dp"
        android:layout_marginTop="18dp"
        android:contentDescription="@string/icon_caption"
        android:src="@drawable/acm_icon" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="@string/no_web_content"
        android:textColor="#FFFFFF"
        android:textSize="25sp" />
    <TextView
        android:id="@+id/acm_description_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="@string/acm_description"
        android:textColor="#FFFFFF"
        android:textSize="20sp" />
    
    <Button
		android:id="@+id/webb_act_bt"
		android:layout_width="match_parent"
		android:layout_height="30dp"
		android:layout_marginTop="10dp"
		android:background="#610B0B"
		android:text="@string/web_bt"
		android:textSize="22sp"
		android:textColor="#FFFFFF"  />

</LinearLayout>